{
  "name": "octo-operations",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run studio\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "studio": "cd sanity-studio && npm run dev",
    "studio:build": "cd sanity-studio && npm run build",
    "studio:deploy": "cd sanity-studio && npm run deploy"
  },
  "dependencies": {
    "@portabletext/svelte": "^3.0.0",
    "@sanity/client": "^7.6.0",
    "@sanity/image-url": "^1.1.0",
    "@sanity/svelte-loader": "^1.13.6",
    "@sanity/visual-editing": "^3.2.4",
    "@tailwindcss/vite": "^4.1.17",
    "groq": "^3.96.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/kit": "^2.14.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.2",
    "@types/eslint": "^9.6.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.13.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte": "^5.2.9",
    "svelte-check": "^4.1.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.3"
  }
}
